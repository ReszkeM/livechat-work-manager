  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "search.exclude": {
    "**/dist/": true,
    "**/.pnp.*": true,
    "**/.yarn": true,
    "**/yarn-error.log": true,
    "**/yarn.lock": true
  }
}
